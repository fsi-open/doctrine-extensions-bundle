{
    "name": "fsi/doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "FSi Doctrine extensions bundle.",
    "keywords": ["fsi", "bundle", "doctrine", "extensions", "doctrine-extensions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "twig/twig": "^2.11",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/twig-bundle": "^3.4|^4.0",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/options-resolver": "^3.4|^4.0",
        "symfony/form" : "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0",
        "doctrine/doctrine-bundle": "^1.2|^2.0",
        "fsi/doctrine-extensions": "^2.0",
        "sensio/framework-extra-bundle": "^5.0",
        "knplabs/knp-gaufrette-bundle": "~0.6"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "friends-of-phpspec/phpspec-expect": "^3.0|^4.0",
        "fsi/datagrid-bundle": "^2.0",
        "phpunit/phpunit": "^7.5|^8.0",
        "phpspec/phpspec": "^5.0|^6.0",
        "phpstan/phpstan": "^0.12.66",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/asset": "^3.4|^4.0",
        "symfony/phpunit-bridge": "^4.3",
        "symfony/security": "^3.4|^4.0",
        "symfony/twig-bridge": "^3.4|^4.0"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\DoctrineExtensionsBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\Bundle\\DoctrineExtensionsBundle\\Tests\\": "Tests/",
            "spec\\FSi\\Bundle\\DoctrineExtensionsBundle\\": "spec/FSi/Bundle/DoctrineExtensionsBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev",
            "2.0": "2.0-dev",
            "1.1": "1.1-dev",
            "1.0": "1.0-dev"
        }
    }
}
