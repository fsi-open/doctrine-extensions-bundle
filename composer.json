{
    "name": "fsi/doctrine-extensions-bundle",
    "type": "symfony-bundle",
    "description": "FSi Doctrine extensions bundle.",
    "keywords": ["fsi", "bundle", "doctrine", "extensions", "doctrine-extensions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@fsi.pl"
        }
    ],
    "repositories": [
        { "type": "composer", "url": "http://git.fsi.pl" }
    ],
    "require": {
        "php": ">=5.3.3",
        "twig/twig": "~1.2",
        "symfony/framework-bundle": "~2.2",
        "symfony/twig-bundle": "~2.2",
        "symfony/http-foundation": "~2.2",
        "symfony/form" : "~2.2",
        "fsi/doctrine-extensions": "0.9.*",
        "knplabs/knp-gaufrette-bundle": "0.2.*@dev",
        "knplabs/gaufrette": "0.2.*@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev",
        "doctrine/orm": "~2.3@dev",
        "symfony/twig-bridge": "~2.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "FSi\\Bundle\\DoctrineExtensionsBundle": "" }
    },
    "target-dir": "FSi/Bundle/DoctrineExtensionsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}